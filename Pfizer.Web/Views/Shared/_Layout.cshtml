<body class="body-flat-background">
    <div class="navbar navbar-inverse box-shadow">
        <div class="container">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-responsive-collapse">
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" href="#"><img class="hidden-sm hidden-xs" src="~/Resources/Images/pfizer_logo.png" /></a>
            <div class="nav-collapse navbar-responsive-collapse in" style="height: auto;">
                <ul class="nav navbar-nav pull-right">
                    @Html.Partial("_MainMenu")                    
                </ul>
            </div>
            <!-- /.nav-collapse -->
        </div>
        <!-- /.container -->
    </div>
    <div class="container" >
        <div class="row" style="height: 100%">
            <div class="col col-lg-12">
                <div class="row">
                    <div class="col col-lg-12">
                        @Html.Partial("_TabPartialView")
                        @RenderBody()
                        @Html.Partial("_GlobalNotification")
