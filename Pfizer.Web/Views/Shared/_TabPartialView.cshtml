@(Html.Kendo().TabStrip()
        .Name("medicard-tabstrip")
        .HtmlAttributes(new { @class = "border-radius" })
        .Items(tabstrip =>
        {
            tabstrip.Add().Text("Home")
                .Selected(true)
                .Content(@<text>
                <iframe id="theFrame_1_0" marginheight="0" seamless="seamless" scrolling="auto" width="100%" height="100%"
                    src='@Url.Content("~/Common/Home/Welcome")' ></iframe>
                </text>);
        })
        .Animation(a => a.Enable(false))
)
